                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="明泉大灯使用灯高调节">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="灯光检测允许在线调整">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="大灯复检左右全部检测">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="摩托车左灯检测">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="正三轮摩托车后轮用摩托车设备检测">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="加载制动不合格允许再检一次">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>

            </dxlc:LayoutGroup>
            <dxlc:LayoutGroup Orientation="Vertical"  Margin="10,0,10,0">
                <dxlc:LayoutItem Label="正三轮摩托车后轮用汽车后板" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="地盘检测结束码" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="大灯检测使用遥控" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="双灯检测不合格允许单灯检测" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="检验报告单套打" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="检验报告单打印" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="打印后印两份" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="仪器设备检验表套打" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
