             mc:Ignorable="d"
             DataContext="{dxmvvm:ViewModelSource Type={x:Type ViewModels:SettingBaseViewModel}}"
             d:DesignHeight="500" d:DesignWidth="500">
    <Grid>

        <dxlc:LayoutControl>
            <dxlc:LayoutGroup Orientation="Vertical" Margin="10,0,10,0">
                <dxlc:LayoutItem Label="最大工位数" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="大灯检测" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="工位控制板数" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="控制板最大通道数" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="汽车轴重下限值" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="摩托车轴重下限值" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="检测站编号" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="检测线代号" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="12">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="制动数据最小字节数">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="华燕轴重仪表返回个数">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="大型车灯高">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="小型车灯高">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>

            </dxlc:LayoutGroup>
            <dxlc:LayoutGroup Orientation="Vertical"  Margin="10,0,10,0">
                <dxlc:LayoutItem Label="工位照片上传方式" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="照片重传次数" >
                    <dxe:TextEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="复检车重新称重" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="滚筒制动/轴重复合仪表" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="滚筒制动/轴重复合仪台" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="轴重台存在中间小板" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="存在平板制动" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="华燕制动台电机保护值" >
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="制动检测失败允许重检">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="低速三轮车前轮手刹检测取轮重">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
                <dxlc:LayoutItem Label="制动检测复位使用遥控器">
                    <dxe:CheckEdit />
                </dxlc:LayoutItem>
